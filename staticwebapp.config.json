{
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "public, max-age=604800"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["*.{js,css,ico,txt}"]
  },
  "mimeTypes": {
    ".webmanifest": "application/manifest+json"
  }
}